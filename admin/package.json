{
  "name": "react-adminlte-admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@tanstack/react-query": "^5.52.0",
    "axios": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "toastr": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "npm --prefix ../backend run start",
    "dev:server": "npm --prefix ../backend run start:dev",
    "build:server": "npm --prefix ../backend run build",
    "dev:client": "vite",
    "dev:all": "concurrently -k -n server,client -c green,blue \"npm:dev:server\" \"npm:dev:client\""
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query-devtools": "^5.52.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "vite": "^4.5.0"
  },
  "overrides": {
    "summernote": "0.8.18"
  }
}
