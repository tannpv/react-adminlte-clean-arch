{
  "name": "admin-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1",
    "@tanstack/query-core": "^5.90.2",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.63.0",
    "react-toastify": "^11.0.5",
    "rollup": "^4.52.3",
    "toastr": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "restart:dev": "pkill -f 'vite' || true && npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "server": "npm --prefix ../backend run start",
    "dev:server": "npm --prefix ../backend run start:dev",
    "build:server": "npm --prefix ../backend run build",
    "dev:client": "vite",
    "dev:all": "concurrently -k -n server,client -c green,blue \"npm:dev:server\" \"npm:dev:client\""
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0"
  }
}
